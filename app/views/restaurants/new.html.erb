<h1>新增餐廳</h1>

<%= form_for(Restaurant.new) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :tel %>
  <%= f.text_field :tel %>

  <%= f.label :mail %>
  <%= f.text_field :mail %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit '新增'%>
<% end %>

